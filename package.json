{
 "name": "command-alias",
 "displayName": "Command Alias",
 "description": "Create aliases for built-in commands",
 "version": "0.4.0",
 "publisher": "ArturoDent",
 "engines": {
  "vscode": "^1.51.0"
 },
 "repository": {
  "type": "git",
  "url": "https://github.com/ArturoDent/command-alias"
 },
 "keywords": [
  "commands",
  "alias",
  "title",
  "rename"
 ],
 "license": "MIT",
 "categories": [
  "Other"
 ],
 "activationEvents": [
  "onStartupFinished",
  "onCommand:command-alias.createAliases",
  "onCommand:command-alias.explorer.newFile.1",
  "onCommand:command-alias.explorer.newFile.2",
  "onCommand:command-alias.explorer.newFolder.1",
  "onCommand:command-alias.git.checkout.1",
  "onCommand:command-alias.workbench.action.terminal.sendSequence.1",
  "onCommand:command-alias.workbench.action.terminal.sendSequence.2"
 ],
 "main": "./src/extension.js",
 "contributes": {
  "commands": [
   {
    "command": "command-alias.createAliases",
    "title": "Create aliases from vscode's built-in commands",
    "category": "Alias"
   },
   {
    "command": "command-alias.explorer.newFile.1",
    "title": "touch",
    "category": "Alias"
   },
   {
    "command": "command-alias.explorer.newFile.2",
    "title": "touch2",
    "category": "Alias"
   },
   {
    "command": "command-alias.explorer.newFolder.1",
    "title": "mkdir",
    "category": "Alias"
   },
   {
    "command": "command-alias.git.checkout.1",
    "title": "Git: Switch to...",
    "category": "Alias"
   },
   {
    "command": "command-alias.workbench.action.terminal.sendSequence.1",
    "title": "open styles",
    "category": "Alias",
    "args": {
     "text": "code -r '../style.scss'\r"
    }
   },
   {
    "command": "command-alias.workbench.action.terminal.sendSequence.2",
    "title": "change terminal directory",
    "category": "Alias",
    "args": {
     "text": "cd '${fileDirname}'\r"
    }
   }
  ],
  "configuration": {
   "title": "Aliases (command labels) created by users",
   "properties": {
    "command aliases": {
     "type": "object",
     "description": "An object containing command and label/alias pairs."
    },
    "commandAlias.category": {
     "type": "string",
     "default": "Alias",
     "description": "A word to precede all created commands, like the default 'Alias' as in 'Alias:  touch' in the Command Palette.",
     "markdownDescription": "A word to precede all created commands in the Command Palette, like the default `Alias` as in `Alias: mkdir`.  Leave empty to use no alias."
    }
   }
  }
 },
 "scripts": {
  "lint": "eslint .",
  "pretest": "npm run lint",
  "test": "node ./test/runTest.js"
 },
 "devDependencies": {
  "@types/vscode": "^1.51.0",
  "@types/glob": "^7.1.1",
  "@types/mocha": "^7.0.2",
  "@types/node": "^13.11.0",
  "eslint": "^6.8.0",
  "glob": "^7.1.6",
  "mocha": "^7.1.2",
  "typescript": "^3.8.3",
  "vscode-test": "^1.3.0"
 },
 "isUserBuiltin": false,
 "isBuiltin": false,
 "isUnderDevelopment": true,
 "id": "ArturoDent.command-alias",
 "identifier": {
  "value": "ArturoDent.command-alias",
  "_lower": "arturodent.command-alias"
 },
 "extensionLocation": {
  "$mid": 1,
  "fsPath": "c:\\Users\\Mark\\command-alias",
  "_sep": 1,
  "path": "/c:/Users/Mark/command-alias",
  "scheme": "file"
 }
}